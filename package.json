{
  "name": "gulp-xml-transformer",
  "version": "5.1.1",
  "description": "Gulp plugin for transforming xml content",
  "main": "dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "build": "rollup -c",
    "test": "mocha",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dotcore64/gulp-xml-transformer.git"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "xml",
    "libxmljs"
  ],
  "author": "Julian Grinblat <julian@dotcore.co.il>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-xml-transformer/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-xml-transformer#readme",
  "dependencies": {
    "arrify": "^2.0.1",
    "libxmljs2": "^0.29.0",
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2",
    "value-or-function": "^3.0.0",
    "vinyl-contents-tostring": "^4.1.1"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "chai": "^4.3.5",
    "chai-as-promised": "^7.1.1",
    "dirname-filename-esm": "^1.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.2.0",
    "p-event": "^5.0.1",
    "rollup": "^2.66.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "vinyl": "^2.2.1"
  }
}
